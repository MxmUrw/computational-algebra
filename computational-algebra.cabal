name: computational-algebra
version: 0.3.0.0
cabal-version: >=1.8
build-type: Simple
license: BSD3
license-file: LICENSE
copyright: (C) Hiromi ISHII 2013
maintainer: konn.jinro_at_gmail.com
homepage: https://github.com/konn/computational-algebra
synopsis: Well-kinded computational algebra library, currently supporting Groebner basis.
description: Dependently-typed computational algebra library for Groebner basis.
category: Math
author: Hiromi ISHII
data-dir: ""
extra-source-files: README.md examples/*.hs tests/*.hs bench/*.hs
                    share/*.hs
 
source-repository head
    type: git
    location: git://github.com/konn/computational-algebra.git
 
flag examples
    Default: False
 
library
    build-depends: MonadRandom ==0.1.13.*, algebra ==3.1.*,
                   base >=4 && <5, bifunctors ==3.2.0.1.*,
                   containers ==0.5.0.0.*, convertible ==1.1.0.0.*,
                   constraints ==0.3.5.*, deepseq ==1.3.0.1.*,
                   equational-reasoning ==0.2.0.2.*, hashable ==1.1.2.5.*,
                   heaps ==0.3.1.*, hmatrix ==0.15.0.1.*, matrix ==0.3.0.0.*,
                   lens ==3.9.2.*, monad-loops ==0.4.2.*, monomorphic ==0.0.3.1.*,
                   mtl ==2.1.2.*, primitive ==0.5.0.1.*, reflection ==1.4.*,
                   representable-functors ==3.2.0.2.*, semigroupoids ==3.1.*,
                   singletons ==0.8.6.*, sized-vector ==1.4.0.0.*, tagged ==0.7.2.*,
                   hybrid-vectors ==0.1.2.*, text ==0.11.3.1.*,
                   type-natural ==0.2.1.0.*, unamb ==0.2.5.*,
                   unordered-containers ==0.2.3.0.*, vector ==0.10.0.1.*,
                   sparse ==0.7.0.1.*, parallel ==3.2.0.3.*, hybrid-vectors ==0.1.2.*,
                   vector-algorithms ==0.5.4.2.*, repa ==3.2.3.3.*,
                   repa-algorithms ==3.2.4.1.*, peggy ==0.3.2.*,
                   control-monad-loop ==0.1.*
    exposed-modules: Algebra.Algorithms.Faugere4
                     Algebra.Algorithms.Faugere5 Algebra.Algorithms.Groebner
                     Algebra.Algorithms.Groebner.Monomorphic Algebra.Algorithms.ZeroDim
                     Algebra.Ring.Ideal Algebra.Ring.Polynomial
                     Algebra.Ring.Polynomial.Quotient
                     Algebra.Ring.Polynomial.Monomorphic Algebra.Ring.Polynomial.Parser
                     Algebra.Scalar Algebra.Internal Algebra.Instances Algebra.Wrapped
                     Algebra.Matrix Algebra.Repa Algebra.Prelude
    exposed: True
    buildable: True
    other-modules: Algebra.Algorithms.FGLM
    ghc-options: -fwarn-name-shadowing -Odph -with-rtsopts="-qa"
 
executable solve
    build-depends: constraints ==0.3.5.*, algebra ==3.1.*,
                   base >=4 && <5, type-natural ==0.2.1.0.*, algebra ==3.1.*,
                   computational-algebra -any, random ==1.0.1.1.*,
                   hmatrix ==0.15.0.1.*, matrix ==0.3.0.0.*, sized-vector ==1.4.0.0.*,
                   vector ==0.10.0.1.*, convertible ==1.1.0.0.*, lens ==3.9.2.*,
                   MonadRandom ==0.1.13.*
     
    if flag(examples)
        buildable: True
    else
        buildable: False
    main-is: solve.hs
    buildable: True
    hs-source-dirs: share examples
    ghc-options: -caf-all -auto-all -rtsopts -O2 -threaded
 
executable ipsolve
    build-depends: parallel ==3.2.0.3.*, constraints ==0.3.5.*,
                   algebra ==3.1.*, equational-reasoning ==0.2.0.2.*,
                   reflection ==1.4.*, base >=4 && <5, type-natural ==0.2.1.0.*,
                   algebra ==3.1.*, computational-algebra -any, random ==1.0.1.1.*,
                   hmatrix ==0.15.0.1.*, matrix ==0.3.0.0.*, sized-vector ==1.4.0.0.*,
